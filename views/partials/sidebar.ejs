<div class="bg-dark border-secondary" id="sidebar-wrapper">
  <div
    style="
      background: url(/img/vb-header.svg) no-repeat;
      width: 240px;
      height: 200px;
    "
    class="sidebar-heading"
  ></div>
  <div class="ml-3 mb-3" id="loggedInUser">
    <img
      style="width: 40px; height: 40px;"
      class="rounded-circle mr-2"
      id="profilePic"
      src="<%= loggedInUserPic %>"
    /><%= loggedInUser %>
  </div>
  <div class="list-group list-group-flush">
    <a
      id="dashboard-nav"
      href="/u/<%= loggedInUser %>/dashboard"
      class="list-group-item list-group-item-action nav-btn text-white"
      ><i class="fas fa-home mr-2"></i>Dashboard<span class="sr-only"></span
    ></a>
    <a
      id="requests-nav"
      href="/u/<%= loggedInUser %>/queue"
      class="list-group-item list-group-item-action nav-btn text-white"
      ><i class="fas fa-music mr-2"></i>Queue<span class="sr-only"></span
    ></a>

    <!-- <a
      id="poll-nav"
